@model SearchPostViewModel

@{
    ViewData["Search"] = $"Showing results for&nbsp<strong>{Model.SearchKeyword}</strong>";
    ViewData["Title"] = $"Search tags for {Model.SearchKeyword} / Connectio";
}

<partial name="_Divider" model=@ViewData["search"] />

@foreach (var post in Model.Posts)
{
    <partial name="_PostCard" model="post" />
}